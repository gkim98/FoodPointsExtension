{
  "manifest_version": 2,
  "name": "Food Points Tracker",
  "description": "This extension tracks your overall food points.",
  "version": "1.0",

  "content_scripts": [
        {
            "matches": [
                "https://my.duke.edu/students/dashboard#continuing/"
            ],
            "js": [
                "assets/js/lib/jquery-3.2.1.min.js",
                "content.js"
            ]
        }
   ],

  "browser_action": {
    "default_popup": "popup.html"
  },

  "background": {
  "scripts": ["testpopup.js"],
  "persistent": false
  },

  "permissions": [

        "storage",
        "notifications",
        "contextMenus"
    ]
}
